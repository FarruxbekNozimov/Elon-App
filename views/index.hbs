{{>header}}
<main class="w-100">
	<section class="py-5 text-center bg-primary position-relative" >
		<div class="row py-lg-5">
			<div class="col-lg-6 col-md-8 mx-auto">
				<h1 class="text-light">Eng so'ngi master klasslar va tadbirlar bizning saytda</h1>
			</div>
		</div>
    <ul class="filterDiv">
      <li>
        <input type="date" name="" id="">
      </li>
      <li>
        <select name="" id="">
          <option>Dasturlash</option>
          <option>SMM</option>
          <option>SMM</option>
          <option>SMM</option>
          <option>SMM</option>
        </select>
      </li>
    </ul>

	</section>


	<div class="album py-5 bg-light">
		<div class="container">
      <h6>Oxirgi e'lonlar</h6>
			<div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 g-3">
				<div class="col">
					<div class="card shadow-sm">
						<svg
							class="bd-placeholder-img card-img-top"
							width="100%"
							height="225"
							xmlns="http://www.w3.org/2000/svg"
							role="img"
							aria-label="Placeholder: Thumbnail"
							preserveAspectRatio="xMidYMid slice"
							focusable="false"
						><title>Placeholder</title><rect
								width="100%"
								height="100%"
								fill="#55595c"
							></rect><text
								x="50%"
								y="50%"
								fill="#eceeef"
								dy=".3em"
							>Thumbnail</text></svg>
						<div class="card-body">
							<p class="card-text">This is a wider card with supporting text
								below as a natural lead-in to additional content. This content
								is a little bit longer.</p>
							<div class="d-flex justify-content-between align-items-center">
								<div class="btn-group">
									<button
										type="button"
										class="btn btn-sm btn-outline-secondary"
									>View</button>
									<button
										type="button"
										class="btn btn-sm btn-outline-secondary"
									>Edit</button>
								</div>
								<small class="text-muted">9 mins</small>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

</main>